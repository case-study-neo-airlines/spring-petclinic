pipeline {

  stages {
    stage('Build-war') {
      steps {
        println "Building WAR file"
      }
    }
    stage('Build-img') {
      steps {
        println "Building Docker image"
      }
    }
    stage('dev-deployment') {
      steps {
        println "Deploying to Dev"
      }
    }
    stage('test-deployment') {
      steps {
        println "Deployingt o test"
      }
    }
  }
  
}
